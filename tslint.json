{
  "extends": "tslint:recommended",
  "rulesDirectory": ["src"],
  "rules": {
    "semicolon": [true, "never"],
    "member-ordering": [false],
    "prefer-const": [false],
    "prefer-for-of": [false],
    "quotemark": [false],
    "trailing-comma": [false],
    "member-access": [false]
